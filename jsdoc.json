{
    "tags": {
        "allowUnknownTags": false
    },
    "recurseDepth": 10,
    "source": {
        "include": ["README.md", "lib/"],
        "includePattern": ".js(doc)?$",
		"excludePattern": "(node_modules/|docs)",
		"recurse": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
	},
	"docdash": {
        "static": false,
        "sort": true
	}
}